-- EXPORTHR.MIS_ORGANIZATION definition

CREATE TABLE "EXPORTHR"."MIS_ORGANIZATION" 
   (	"ORG_ID" VARCHAR2(32) NOT NULL ENABLE, 
	"ORG_ID_OLD" VARCHAR2(32), 
	"PARENT_ORG_ID" VARCHAR2(32), 
	"ORG_PATH_CODE" VARCHAR2(512), 
	"ORG_DESC_EN" VARCHAR2(999), 
	"ORG_DESC_ZH" VARCHAR2(999), 
	"ORG_CHI_NAME" VARCHAR2(210), 
	"ORG_ENG_NAME" VARCHAR2(128), 
	"ORG_LEVEL" VARCHAR2(40), 
	"ORG_GRADE" VARCHAR2(12), 
	"COMPANY_CODE" VARCHAR2(32), 
	"COMPANY_NAME" VARCHAR2(60), 
	"COST_CENTER_CODE" VARCHAR2(20), 
	"ORG_BEGDA" DATE, 
	"ORG_ENDDA" DATE, 
	"ORG_DOA" VARCHAR2(2), 
	"SPECIAL_PARENT_ORG_ID" VARCHAR2(240), 
	"ORG_SORT" VARCHAR2(20), 
	"ORG_HEAD_COUNT" VARCHAR2(20), 
	"ORG_ACTL_COUNT" VARCHAR2(20), 
	"ORG_SHORT_CHI_NAME" VARCHAR2(150), 
	"ORG_SHORT_ENG_NAME" VARCHAR2(150)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 8192 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "BASICDATAUSER_TBS" ;

GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "BASICDATAUAT";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "HCMUSER";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "PRODBASICDATAUSER";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "WXICMPMSADM";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "EPMMISIT";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "YOYOSERVER";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "MK_EXPORTHR_READ";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "ONLINE_EXPORTHR_READ";
  GRANT SELECT ON "EXPORTHR"."MIS_ORGANIZATION" TO "EXPORTHR_READ";

COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_ID IS '组织ID';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_ID_OLD IS '老组织ID';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.PARENT_ORG_ID IS '父组织ID';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_PATH_CODE IS '组织层级路径,按组织Code展示的路径';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_DESC_EN IS '组织英文全称,带组织层级路径';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_DESC_ZH IS '组织中文全称,带组织层级路径';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_CHI_NAME IS '组织中文名称';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_ENG_NAME IS '组织英文名称';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_LEVEL IS '组织级别';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_GRADE IS '组织级别 100-SUBLINE
200-LINE
300-SECTION
350-SUBDEPARTMENT
400-DEPARTMENT
450-SUBDIVISION
500-DIVISION
600-CENTER
750-SUBMANAGEMENT
1000-COMPANY';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.COMPANY_CODE IS '公司编码';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.COMPANY_NAME IS '公司名称';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.COST_CENTER_CODE IS '成本中心编码';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_BEGDA IS '组织生效开始时间';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_ENDDA IS '组织生效结束时间';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_DOA IS '组织层级 00-公司 10一级部门 20-二级部门 30-三级部门 40-四级部门';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.SPECIAL_PARENT_ORG_ID IS '特殊父组织ID 目前暂未用到';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_SORT IS '组织排序';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_SHORT_CHI_NAME IS '组织简写中文名称，目前暂未用到';
COMMENT ON COLUMN EXPORTHR.MIS_ORGANIZATION.ORG_SHORT_ENG_NAME IS '组织简写英文名称，目前暂未用到';



-- EXPORTHR.MIS_EMPLOYEE definition

CREATE TABLE "EXPORTHR"."MIS_EMPLOYEE" 
   (	"EMPLOYEE_NO" VARCHAR2(32) NOT NULL ENABLE, 
	"EMPLOYEE_NO_SAP" VARCHAR2(32), 
	"ORG_ID" VARCHAR2(32), 
	"LOCAL_F_NAME" VARCHAR2(120), 
	"LOCAL_L_NAME" VARCHAR2(120), 
	"ENGLISH_F_NAME" VARCHAR2(20), 
	"ENGLISH_L_NAME" VARCHAR2(60), 
	"PREFERRED_NAME" VARCHAR2(60), 
	"GENDER" VARCHAR2(8), 
	"EMAIL" VARCHAR2(120), 
	"PHONE" NVARCHAR2(241), 
	"MOBILE_PHONE" VARCHAR2(120), 
	"EMPLOYEE_BIRTHDAY" DATE, 
	"EMPLOYEE_QULIFICATION" VARCHAR2(60), 
	"IS_PARTY" VARCHAR2(1), 
	"PARTY_TIME" DATE, 
	"IS_MANAGER" VARCHAR2(1), 
	"EMPLOYEE_DEGREE" VARCHAR2(60), 
	"SCOPE" VARCHAR2(60), 
	"SCOPE_NAME" VARCHAR2(60), 
	"SUB_SCOPE" VARCHAR2(60), 
	"SUB_SCOPE_NAME" VARCHAR2(60), 
	"EMPLOYEE_GROUP" VARCHAR2(60), 
	"EMPLOYEE_GROUP_NAME" VARCHAR2(60), 
	"EMPLOYEE_SUB_GROUP" VARCHAR2(60), 
	"EMPLOYEE_SUB_GROUP_NAME" VARCHAR2(60), 
	"JOB_ID" VARCHAR2(20), 
	"JOB_CHI_TITLE" VARCHAR2(180), 
	"JOB_ENG_TITLE" VARCHAR2(120), 
	"POSITION_ID" VARCHAR2(20), 
	"POSITION_CHI_TITLE" VARCHAR2(180), 
	"POSITION_ENG_TITLE" VARCHAR2(120), 
	"STATUS" VARCHAR2(60), 
	"IN_OUT_BOUND" VARCHAR2(20), 
	"DATE_ENTRY" VARCHAR2(20), 
	"DATE_LEAVE" VARCHAR2(20), 
	"SERVICE_PERIOD" VARCHAR2(20), 
	"NATION" VARCHAR2(20), 
	"E_COMPANY_CODE" VARCHAR2(20), 
	"E_COMPANY_NAME" VARCHAR2(20), 
	"E_COST_CENTER_CODE" VARCHAR2(20), 
	"WORK_PLACE" VARCHAR2(20), 
	"COMPANY_MOBILE" NVARCHAR2(241), 
	"COMPANY_LINE" NVARCHAR2(241), 
	"EMPLOYEE_OUT_GROUP" VARCHAR2(60), 
	"EMPLOYEE_OUT_GROUP_NAME" VARCHAR2(60), 
	"PROXY_EMPLOYEE" VARCHAR2(16), 
	"PROXY_START_DATE" VARCHAR2(16), 
	"OLD_EMPLOYEE_NO" VARCHAR2(16), 
	"NEW_EMPLOYEE_NO_EFFECTIVE_DATE" VARCHAR2(32), 
	"IS_REGULAR" VARCHAR2(8), 
	"POSITIVE_DATE" VARCHAR2(16), 
	"EMPLOYEE_DEGREE_RANK" VARCHAR2(200)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 8192 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "BASICDATAUSER_TBS" ;

GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "BASICDATAUAT";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "PRODBASICDATAUSER";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "HCMUSER";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "WXICMPMSADM";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "EPMMISIT";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "YOYOSERVER";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "MK_EXPORTHR_READ";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "ONLINE_EXPORTHR_READ";
  GRANT SELECT ON "EXPORTHR"."MIS_EMPLOYEE" TO "EXPORTHR_READ";

COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_NO IS '员工工号';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_NO_SAP IS '在SAP中员工ID';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.ORG_ID IS '员工所属组织ID';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.LOCAL_F_NAME IS '员工中文姓';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.LOCAL_L_NAME IS '员工中文名';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.ENGLISH_F_NAME IS '员工英文姓';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.ENGLISH_L_NAME IS '员工英文名';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.PREFERRED_NAME IS '员工昵称,由员工英文名+员工英文姓组成，英文名在前面，姓氏在后面';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.GENDER IS '员工性别, f-女, m-男';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMAIL IS '员工邮箱';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.PHONE IS '员工电话,一般是员工办公座机电话';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.MOBILE_PHONE IS '员工个人移动电话';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_BIRTHDAY IS '员工生日';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_QULIFICATION IS '员工学历资格';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.IS_PARTY IS '是否党员, 该字段目前没用';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.PARTY_TIME IS '入党时间, 该字段目前没用';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.IS_MANAGER IS '是否组织管理者';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_DEGREE IS '员工职级';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE."SCOPE" IS '员工人事范围 1000-YMTC, 1100-XMC, 1700-YMBJ';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.SCOPE_NAME IS '员工人事范围名称 XMC, YMCT';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.SUB_SCOPE IS '员工人事子范围 1001-研发 1002-工程技术类 1003-管理支持类 1004-高级管理类';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.SUB_SCOPE_NAME IS '员工人事子范围名称  研发,工程技术类,管理支持类,高级管理类';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_GROUP IS '员工组 1-正式员工 2-实习生 3-咨询顾问 4-厂商 5-外聘员工 6-劳务派遣员工';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_GROUP_NAME IS '员工组名称 Employees-正式员工, Intern-实习生, Consultant-咨询顾问 Temporary staff-厂商 External staff-外聘员工 Labor dispatch-劳务派遣';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_SUB_GROUP IS '员工子组 
10-O.B High-level Mange
11-I.B High-level Mange
12-O.B Ordinary Employe
13-I.B Ordinary Employe
14-TA
15-MA
20-Monthly salary
21-Daily salary
30-On Business
31-On person
32-Bilateral
40-Monthly salary
43-Unpaid
50-Hire back by Retired
60-Labor dispatch';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_SUB_GROUP_NAME IS '员工子组名称 ';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.JOB_ID IS '员工职位编码';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.JOB_CHI_TITLE IS '员工职位中文名称';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.JOB_ENG_TITLE IS '员工职位英文名称';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.POSITION_ID IS '员工岗位编码';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.POSITION_CHI_TITLE IS '员工岗位中文名称';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.POSITION_ENG_TITLE IS '员工岗位英文名称';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.STATUS IS '员工在岗状态, 0--离职， 3-在职';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.IN_OUT_BOUND IS '境内外 I-境内 O-境外';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.DATE_ENTRY IS '员工入职时间';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.DATE_LEAVE IS '员工离职时间';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.SERVICE_PERIOD IS '员工在公司工作时长';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.NATION IS '员工国籍';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.E_COMPANY_CODE IS '员工所属公司编码';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.E_COMPANY_NAME IS '员工所属公司名称';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.E_COST_CENTER_CODE IS '员工成本核算中心编码';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.WORK_PLACE IS '员工工作地';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.COMPANY_MOBILE IS '员工办公移动电话';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.COMPANY_LINE IS '员工办公电话';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_OUT_GROUP IS '外部员工组 1-Unigroup Assignment 4-Vendor 7';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_OUT_GROUP_NAME IS '外部员工组';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.PROXY_EMPLOYEE IS '代理员工';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.PROXY_START_DATE IS '代理开始时间';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.OLD_EMPLOYEE_NO IS '旧员工工号';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.NEW_EMPLOYEE_NO_EFFECTIVE_DATE IS '新员工工号生效时间';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.IS_REGULAR IS '是否正式员工 X-表示正式员工';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.POSITIVE_DATE IS '当前岗位调整时间,针对岗位有变动员工';
COMMENT ON COLUMN EXPORTHR.MIS_EMPLOYEE.EMPLOYEE_DEGREE_RANK IS '员工职级小类';












